<%-include('./partials/header') %>
<header class="masthead" style="background-image: url('https://cdn.pixabay.com/photo/2022/02/20/20/09/field-7025238_960_720.jpg')">
    <div class="container position-relative px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
            <div class="col-md-10 col-lg-8 col-xl-7">
                <div class="site-heading">
                    <h1>Clean Blog</h1>
                    <span class="subheading">A Blog Theme by Start Bootstrap</span>
                </div>
            </div>
        </div>
    </div>
</header>
<div class="container px-4 px-lg-5">

    <% data.forEach( (item) => { %>
       
         

    <div class="row gx-4 gx-lg-5 justify-content-center">

        <div class="col-sm-4 col-md-3 col-lg-4 col-xl-4 mx-auto">
            <img class="img-thumbnail" src="<%=item.image%>">
        </div>

        <div class="col-sm-8 col-md-10 col-lg-8 col-xl-8">
            <!-- Post preview-->
            <div class="post-preview">
                <a href="post.html">
                    <h2 class="post-title"> <%=item.postTitle%></h2>
                    <h3 class="post-subtitle"><%=item.postSubtitle%></h3>
                </a>
                <p class="post-meta">
                    Posted by
                    <a href="#!">Start Bootstrap</a>
                    on September 24, 2021
                </p>
            </div>
            <!-- Divider-->
            <hr class="my-4" />
            
            
            <!-- Pager-->
            
        </div>
    </div>
    <%});%>
    <div class="d-flex justify-content-end mb-4"><a class="btn btn-primary text-uppercase" href="#!">Older Posts â†’</a></div>
</div>

<%-include('./partials/footer')%>